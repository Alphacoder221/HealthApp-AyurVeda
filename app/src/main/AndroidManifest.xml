<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mrkunalsinghal.myapplication">

    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/care"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".showdatabase" />
        <activity android:name=".loginpageactivity" />
        <activity android:name=".signup" />
        <activity android:name=".round" />
        <activity android:name=".homepage1" />
        <activity android:name=".profilepage" />
        <activity android:name=".doctordatabase" />
        <activity android:name=".doctorsignup" />
        <activity android:name=".setappointment" />
        <activity android:name=".appointmentdb" />
        <activity android:name=".showappointment" />
        <activity android:name=".AndroidGPSTrackingActivity" />
        <activity android:name=".MainMedicine" />
        <activity android:name=".AddMadicineDetail" />
        <activity android:name=".Notfication" />

        <receiver android:name=".TimeAlarm">
            <intent-filter>
                <action android:name="android.media.action.DISPLAY_NOTIFICATION" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </receiver>

    </application>

</manifest>